# lulzprime git attributes

# Auto detect text files and normalize line endings
* text=auto

# Source code
*.py text eol=lf
*.pyi text eol=lf

# Documentation
*.md text eol=lf
*.txt text eol=lf
*.rst text eol=lf

# Configuration
*.toml text eol=lf
*.yaml text eol=lf
*.yml text eol=lf
*.json text eol=lf
*.ini text eol=lf
*.cfg text eol=lf

# Scripts
*.sh text eol=lf
*.bash text eol=lf

# Binary files
*.pdf binary
*.png binary
*.jpg binary
*.jpeg binary
*.gif binary
*.ico binary
*.woff binary
*.woff2 binary
*.ttf binary
*.eot binary
